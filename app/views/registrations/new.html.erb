<h1>Sign up</h1>

<%= form_with model: @user, url: registrations_path do |form| %>
  <div><%= form.label :email %></div>
  <div><%= form.email_field :email %></div>

  <div><%= form.label :password %></div>
  <div><%= form.password_field :password %></div>

  <%= form.submit "Sign up" %>
<% end %>

<%= link_to  "Log in", new_login_path %>
